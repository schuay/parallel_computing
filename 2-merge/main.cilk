#include <cilk-lib.cilkh>
#include <stdio.h>
#include <unistd.h>

cilk static void hello(void) {
    printf("hello self %d\n", Self);
    usleep(100);
}

cilk int main(int argc, char **argv) {
    int i;

    printf("Cilk_active_size: %d\n", Cilk_active_size);

    for (i = 0; i < Cilk_active_size; i++) {
        spawn hello();
    }
    return 0;
}
